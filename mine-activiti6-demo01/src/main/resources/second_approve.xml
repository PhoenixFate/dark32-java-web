  <process id="second_approve" name="二级审批" isExecutable="true">
    <startEvent id="startEvent" name="开始节点">
      <extensionElements>
        <activiti:formProperty id="Property 1"/>
      </extensionElements>
    </startEvent>
    <userTask id="submitForm" name="填写审批信息">
      <extensionElements>
        <activiti:formProperty id="message" name="申请信息" type="String"/>
        <activiti:formProperty id="name" name="申请人名称" type="String"/>
        <activiti:formProperty id="submitTime" name="提交时间" type="Date" datePattern="yyyy-MM-dd"/>
        <activiti:formProperty id="submitType" name="确认申请" type="String"/>
      </extensionElements>
    </userTask>
    <sequenceFlow id="sid-b1d58f81-6428-4292-a4a2-e8a39e8a8bd0" sourceRef="startEvent" targetRef="submitForm"/>
    <exclusiveGateway id="decideSubmit" name="提交或者取消"/>
    <sequenceFlow id="sid-08fc8f5f-63ea-4e89-bb47-b35dbc4a5716" sourceRef="submitForm" targetRef="decideSubmit"/>
    <userTask id="teamLeaderApprove" name="主管审批">
      <extensionElements>
        <activiti:formProperty id="teamLeaderApproveResult" name="主管审批结果" type="String"/>
        <activiti:formProperty id="teamLeaderApproveMessage" name="主管审批备注" type="String"/>
      </extensionElements>
    </userTask>
    <sequenceFlow id="sid-53e77f93-53b7-4cc0-864c-d7920f364777" sourceRef="decideSubmit" targetRef="teamLeaderApprove">
      <conditionExpression xsi:type="$(submitType==&quot;y&quot;||submitType==&quot;Y&quot;)"/>
    </sequenceFlow>
    <exclusiveGateway id="teamLeaderDecideApprove" name="主管审批校验"/>
    <sequenceFlow id="sid-9aa4ab8f-1341-4859-9ae6-dbebb45c6767" sourceRef="teamLeaderApprove" targetRef="teamLeaderDecideApprove"/>
    <userTask id="humanResourceApprove" name="人事审批">
      <extensionElements>
        <activiti:formProperty id="humanResourceApproveResult" name="人事审批结果" type="String">
          <activiti:value id="Property value 1"/>
        </activiti:formProperty>
        <activiti:formProperty id="humanResourceApproveMessage" name="人事审批备注" type="String"/>
      </extensionElements>
    </userTask>
    <sequenceFlow id="sid-7cdf412c-3488-4a32-9d28-836d538fde46" sourceRef="teamLeaderDecideApprove" targetRef="humanResourceApprove">
      <conditionExpression xsi:type="$(teamLeaderApproveResult==&quot;y&quot;||teamLeaderApproveResult==&quot;Y&quot;)"/>
    </sequenceFlow>
    <exclusiveGateway id="humanResourceDecideApprove" name="人事审批校验"/>
    <sequenceFlow id="sid-38c01dda-5588-4e1d-a913-79b8c9ee3fc4" sourceRef="humanResourceApprove" targetRef="humanResourceDecideApprove"/>
    <endEvent id="endEvent" name="结束节点"/>
    <endEvent id="cancelEvent" name="取消节点">
      <cancelEventDefinition/>
    </endEvent>
    <sequenceFlow id="sid-2f716324-f56e-4315-9039-9822dc307be2" sourceRef="decideSubmit" targetRef="cancelEvent">
      <conditionExpression xsi:type="$(submitType==&quot;n&quot;||submitType==&quot;N&quot;)"/>
    </sequenceFlow>
    <sequenceFlow id="sid-ad631ae2-0f11-42f2-9d40-a338ac866fde" sourceRef="teamLeaderDecideApprove" targetRef="submitForm">
      <conditionExpression xsi:type="$(teamLeaderApproveResult==&quot;y&quot;||teamLeaderApproveResult==&quot;Y&quot;)"/>
    </sequenceFlow>
    <sequenceFlow id="sid-5a41bcbb-ff84-4131-b7a7-be3cde23ac1d" sourceRef="humanResourceDecideApprove" targetRef="submitForm">
      <conditionExpression xsi:type="$(humanResourceApproveResult==&quot;n&quot;||humanResourceApproveResult==&quot;N&quot;)"/>
    </sequenceFlow>
    <sequenceFlow id="sid-7c32580f-5bcc-4e76-8d73-d416e24ddeba" sourceRef="humanResourceDecideApprove" targetRef="endEvent">
      <conditionExpression xsi:type="$(humanResourceApproveResult==&quot;y&quot;||humanResourceApproveResult==&quot;Y&quot;)"/>
    </sequenceFlow>
  </process>
